{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "strictNullChecks": true,
        "noUnusedParameters": true,
        "declaration": true,
        "outDir": "./lib",
        "resolveJsonModule": true,
        "sourceMap": false,
    },
    "include": ["src"],
    "exclude": [
        "node_modules"
    ]
}
